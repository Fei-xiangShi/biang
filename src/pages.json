{
  "easycom": {
    "custom": {
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "主页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/classTable/classTable",
      "style": {
        "navigationBarTitleText": "课表",
        "navigationStyle":"custom"
      }
    },
    {
      "path" : "pages/classDetail/classDetail",
      "style": {
        "navigationBarTitleText": "课程详情",
        "navigationStyle":"custom"
      }
    }
  ],
  "subPackages":[
    {
      "root":"pages/my/sub",
      "pages":[
        {
          "path":"changeClassTableUrl",
          "style":{
            "navigationBarTitleText": "绑定课表",
            "navigationStyle": "custom"
          }
        },
        {
          "path":"settings",
          "style":{
            "navigationBarTitleText": "设置",
            "navigationStyle": "custom"
          }
        },
        {
          "path":"verifyEmail",
          "style":{
            "navigationBarTitleText": "邮箱绑定",
            "navigationStyle": "custom"
          }
        },
        {
          "path":"verifyCode",
          "style":{
            "navigationBarTitleText": "验证码输入",
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root":"pages/admin/notices",
      "pages":[
        {
          "path":"notices",
          "style":{
            "navigationBarTitleText": "通知管理",
            "navigationStyle": "custom"
          }
        },
        {
          "path":"add",
          "style":{
            "navigationBarTitleText": "添加通知",
            "navigationStyle": "custom"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#e34969",
    "borderStyle": "black",
    "backgroundColor": "#FFFFFF",
    "iconWidth": "24px",
    "list": [{
        "pagePath": "pages/index/index",
        "iconPath": "static/icons/index/index_unselected.png",
        "selectedIconPath": "static/icons/index/index_selected.png",
        "text": "主页"
      },
      {
        "pagePath": "pages/classTable/classTable",
        "iconPath": "static/icons/classTable/classTable_unselected.png",
        "selectedIconPath": "static/icons/classTable/classTable_selected.png",
        "text": "课表"
      },
      {
        "pagePath": "pages/my/my",
        "iconPath": "static/icons/my/my_unselected.png",
        "selectedIconPath": "static/icons/my/my_selected.png",
        "text": "我的"
      }
    ]
  }
}


